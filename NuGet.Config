<?xml version="1.0" encoding="utf-8"?>

<configuration>
	<packageSources>
		<add key="github" value="https://nuget.pkg.github.com/byr0n3/index.json" />
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
	</packageSources>
	<packageSourceMapping>
		<packageSource key="github">
			<package pattern="Elegance.*" />
			<package pattern="Vapid.NET" />
		</packageSource>
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
	</packageSourceMapping>
</configuration>
